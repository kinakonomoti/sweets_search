<h2>マイページ編集</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">名前</label>
  <p><%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%></p>
  <p><label for="inputImage">ProfileImage</label><br>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %></p>  
  <%= f.submit "変更する" %>
<% end %>
